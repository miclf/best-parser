{
    "name": "miclf/best-parser",
    "description": "A parser for the open data from the federal Belgian Street Address project",
    "type": "project",
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/database": "^8.40",
        "laravel-zero/framework": "^8.8",
        "proj4php/proj4php": "^2.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "mockery/mockery": "^1.4.3",
        "pestphp/pest": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["application"]
}
